varnishtest "Enhance a backend"

varnish v1 -vcl {
	import fsdirector from "${vmod_topbuild}/src/.libs/libvmod_fsdirector.so" ;

	backend static {
		.host = "127.0.0.1";
		.port = "8080";
	}

	sub vcl_init {
		new fs = fsdirector.file_system(static);
	}
} -start

varnish v1 -cliok "backend.list"
varnish v1 -wait

