varnishtest "Request a file"

varnish v1 -vcl {
	import fsdirector from "${vmod_topbuild}/src/.libs/libvmod_fsdirector.so" ;

	backend static {
		.host = "127.0.0.1";
		.port = "8080";
	}

	sub vcl_init {
		new fs = fsdirector.file_system(static);
	}
} -start

# XXX not implmented yet :)
varnish v1 -cliok "param.set first_byte_timeout 1"

client c1 {
	txreq
	rxresp
	expect resp.status == 503
} -run

