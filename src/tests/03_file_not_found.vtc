varnishtest "File Not Found"

varnish v1 -vcl {
	import fsdirector from "${vmod_topbuild}/src/.libs/libvmod_fsdirector.so" ;

	backend static {
		.host = "127.0.0.1";
		.port = "8080";
	}

	sub vcl_init {
		new fs = fsdirector.file_system(static, "${vmod_topbuild}");
	}
} -start

client c1 {
	txreq -url "/src/tests/www/index.htm"
	rxresp
	expect resp.status == 404
} -run
